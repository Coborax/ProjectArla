<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox minHeight="720.0" minWidth="1280.0" styleClass="mainContent" stylesheets="@../../../css/main.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.redheads.arla.ui.controllers.UserController">
   <children>
      <GridPane fx:id="tileGrid" alignment="CENTER" hgap="10.0" vgap="10.0" VBox.vgrow="ALWAYS">
      </GridPane>
      <HBox alignment="CENTER_RIGHT" prefHeight="41.0" prefWidth="1280.0" spacing="10.0">
         <children>
            <JFXButton onAction="#refresh" text="Refresh" />
            <JFXButton onAction="#logout" text="Logout" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
