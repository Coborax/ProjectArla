<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" minHeight="720.0" minWidth="1280.0" styleClass="mainContent" stylesheets="@../../../css/main.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.redheads.arla.ui.controllers.UserController">
   <children>
      <GridPane fx:id="tileGrid" alignment="CENTER" hgap="10.0" vgap="10.0" VBox.vgrow="ALWAYS">
      </GridPane>
      <VBox fx:id="fullscreenContainer" alignment="CENTER" VBox.vgrow="ALWAYS" />
      <VBox fx:id="messageContainer" alignment="CENTER" prefHeight="26.0" prefWidth="1300.0" styleClass="critical">
         <children>
            <Label fx:id="messageField" text="Admin Message" textAlignment="CENTER" textFill="WHITE" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
      <HBox alignment="CENTER_RIGHT" prefHeight="41.0" prefWidth="1280.0" spacing="10.0">
         <children>
            <JFXButton onAction="#refresh" text="Refresh" />
            <JFXButton onAction="#logout" text="Logout" />
         </children>
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </HBox>
   </children>
</VBox>
